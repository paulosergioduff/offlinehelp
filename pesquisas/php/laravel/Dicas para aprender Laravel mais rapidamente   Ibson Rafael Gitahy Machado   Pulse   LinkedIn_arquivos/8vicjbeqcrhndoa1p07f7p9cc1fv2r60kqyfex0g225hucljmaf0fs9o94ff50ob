true},typeaheadClosed:function(){this.$mentionsTypeahead.open=false},typeaheadSelected:function(evt,data,type){var curValue=this.$mentionsInput.val(),curMatch=this.lastMatch.match,start=curMatch.start,itemType,newValue,tmpResult,displayToken=htmlUnencode(data.displayName),isFirstDegree=data.misc&&data.misc.degree?true:false,tokenLen=displayToken.length,triggerIncluded=curMatch.triggerIncluded,types={Companies:"company",People:"member",Universities:"school"};_.extend(curMatch,data);if(start)start++;
